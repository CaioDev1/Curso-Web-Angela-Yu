<%- include('header') %>
    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>
    <div class="box">
        <% for(itens in newListItem) { %>
           <div class="item">
              <input type="checkbox">
              <p> <%= newListItem[itens] %> </p>
           </div>
        <% } %>
        <form action="<%= page %>" method="POST">
            <input type="text" name="newItem" id="text" required placeholder="Type something" autocomplete="off">
            <button type="submit" name="button">+</button>
        </form>
    </div>
<%- include('footer') %>
